                    <apex:column value="{!wr.accName.ClosedWonTotal__c}"/>
                    <!--  <apex:column value="{!wr.Phone}" />-->
                </apex:pageBlockTable>
            </apex:pageBlockSection>
        </apex:pageBlock>
